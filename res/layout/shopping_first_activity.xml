<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include layout="@layout/title" />

    <ImageView
        android:id="@+id/first_title_image"
        android:layout_width="wrap_content"
        android:layout_height="180dp"
        android:src="@drawable/u9" />
    <LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="15dp"
	    android:layout_marginTop="5dp"
	    android:orientation="vertical"
	    >
	    <ImageView 
	        android:id="@+id/first_image_tianmao"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/cat"
	        />
	    <TextView 
	        android:id="@+id/first_text_tianmao"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="#FFFFFF"
	        android:layout_marginTop="5dp"
	        android:textSize="12dp"
	        android:text="天猫"
	        />
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="40dp"
	    android:layout_marginTop="5dp"
	    android:orientation="vertical"
	    >
	    <ImageView 
	        android:id="@+id/first_image_ju"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/ju"
	        />
	    <TextView 
	        android:id="@+id/first_text_ju"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="#FFFFFF"
	        android:layout_marginTop="5dp"
	         android:textSize="12dp"
	        android:text="聚划算"
	        />
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="40dp"
	    android:layout_marginTop="5dp"
	    android:orientation="vertical"
	    >
	    <ImageView 
	        android:id="@+id/first_image_ding"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/ding"
	        />
	    <TextView 
	        android:id="@+id/first_text_ding"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="#FFFFFF"
	        android:layout_marginTop="5dp"
	         android:textSize="12dp"
	        android:text="淘生活"
	        />
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="40dp"
	    android:layout_marginTop="5dp"
	    android:orientation="vertical"
	    >
	    <ImageView 
	        android:id="@+id/first_image_dian"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/dian"
	        />
	    <TextView 
	        android:id="@+id/first_text_dian"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="#FFFFFF"
	        android:layout_marginTop="5dp"
	         android:textSize="12dp"
	        android:text="淘点点"
	        />
	</LinearLayout>
    </LinearLayout>
	<LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="15dp"
	    android:layout_marginTop="5dp"
	    android:orientation="vertical"
	    >
	    <ImageView 
	        android:id="@+id/first_image_phone"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/phone"
	        />
	    <TextView 
	        android:id="@+id/first_text_phone"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="#FFFFFF"
	        android:layout_marginTop="5dp"
	         android:textSize="12dp"
	        android:text="充值"
	        />
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="40dp"
	    android:layout_marginTop="5dp"
	    android:orientation="vertical"
	    >
	    <ImageView 
	        android:id="@+id/first_image_cai"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/cai"
	        />
	    <TextView 
	        android:id="@+id/first_text_cai"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="#FFFFFF"
	        android:layout_marginTop="5dp"
	         android:textSize="12dp"
	        android:text="彩票"
	        />
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="40dp"
	    android:layout_marginTop="5dp"
	    android:orientation="vertical"
	    >
	    <ImageView 
	        android:id="@+id/first_image_tao"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/tao"
	        />
	    <TextView 
	        android:id="@+id/first_text_tao"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="#FFFFFF"
	        android:layout_marginTop="5dp"
	         android:textSize="12dp"
	        android:text="领金币"
	        />
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="40dp"
	    android:layout_marginTop="5dp"
	    android:orientation="vertical"
	    >
	    <ImageView 
	        android:id="@+id/first_image_more"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/more"
	        />
	    <TextView 
	        android:id="@+id/first_text_more"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textColor="#FFFFFF"
	        android:layout_marginTop="5dp"
	         android:textSize="12dp"
	        android:text="类目"
	        />
	</LinearLayout>
    </LinearLayout>
    <LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
    
    <TextView 
        android:id="@+id/first_catguoji"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        android:text=".天猫国际"
        />
    <TextView 
        android:id="@+id/first_pinpaijie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        android:text=".品牌街"
        />
    <TextView 
        android:id="@+id/first_miaoxiansheng"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        android:text=".喵鲜生"
        />
    <TextView 
        android:id="@+id/first_taobaodianying"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        android:text=".淘宝电影"
        />
    </LinearLayout>
    <LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <TextView 
        android:id="@+id/first_qingca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        android:text=".清仓"
        />
        <TextView 
        android:id="@+id/first_weeked_taobao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        android:text=".周末淘宝"
        />
        <TextView 
        android:id="@+id/first_taobao_zhongchou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        android:text=".淘宝众筹"
        />
        <TextView 
        android:id="@+id/first_luxing_tehui"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
        android:textColor="#FFFFFF"
        android:textSize="12dp"
        android:text=".旅行特惠"
        />
    </LinearLayout>
</LinearLayout>